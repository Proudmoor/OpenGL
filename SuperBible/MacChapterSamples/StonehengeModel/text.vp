// text.vp
// Stonehenge text vertex shader
#version 150

uniform mat4 mModelViewProjectionMatrix;

in vec4 vVertexPos;
in vec2 vTextCoord;

out vec2 vTexCoordVary;

void main(void)
   {
   vTexCoordVary = vTextCoord;
   
   gl_Position = mModelViewProjectionMatrix * vVertexPos;
   }